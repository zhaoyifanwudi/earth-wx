page {
  width: 100%;
  height: 100%;
}
.header{
  width: 100%;
  height: 40px;
  /* border-top: 1px solid #e0e0e0; */
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-left: 30px;
}
.header-left{
  height: 40px;
  width: 300px;
  display: flex;
  align-items: center;
}

.header-icon{
  height: 40px;
  line-height: 40px;
  width: 25px;
  text-align: center;
}
.header-icon text{
  width: 25px;
  font-size: 20px;
}
.header input{
  box-sizing: border-box;
  width: 200px;
  height: 40px;
}
.header-right{
  margin-right: 30px;
  height: 40px;
  line-height: 40px;
  width: 50px;
}
.header-right text{
  height: 40px;
  width: 40px;
  font-size: 25px;
}



